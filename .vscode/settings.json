{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true
  },
  "intelephense.environment.phpVersion": "8.2.0",
  "phpcs.enable": true,
  "phpcs.standard": "./vendor/wp-coding-standards/wpcs/WordPress",
  "phpsab.snifferEnable": true,
  "phpsab.snifferMode": "onSave",
  "phpsab.standard": "./vendor/wp-coding-standards/wpcs/WordPress",
  "phpsab.executablePathCS": "./vendor/bin/phpcs",
  "phpsab.executablePathCBF": "./vendor/bin/phpcbf",
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client",
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  },
  "css": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  },
  "[less]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  },
  "cSpell.words": [
    "ABSPATH",
    "acerca",
    "Actualizar",
    "Agregar",
    "Atributos",
    "Atrributos",
    "Autoloader",
    "bloginfo",
    "Buscar",
    "Cargado",
    "Caso",
    "casos",
    "categoria",
    "Categoría",
    "categorias",
    "Categorias",
    "Categorías",
    "Certificado",
    "certificados",
    "Certificados",
    "Colocar",
    "dashicons",
    "Destacada",
    "Editar",
    "Emisión",
    "emisor",
    "Emisor",
    "Escoger",
    "Éxito",
    "Fecha",
    "Filtro",
    "gtmcomp",
    "gtmevent",
    "imagen",
    "Imagen",
    "Insertar",
    "kses",
    "Listado",
    "metabox",
    "Metabox",
    "metaboxes",
    "Metaboxes",
    "NOODP",
    "Nueva",
    "Nuevo",
    "Papelera",
    "pingback",
    "Populares",
    "postdata",
    "resultados",
    "robertochoa",
    "Separar",
    "servicio",
    "Servicio",
    "Servicios",
    "subpackage",
    "tagcloud",
    "tailpress",
    "tecnologia",
    "Tecnología",
    "Tecnologias",
    "tipo",
    "Todas",
    "Todos",
    "usados",
    "Usar",
    "Yoast"
  ],
  "[groovy]": {
    "editor.defaultFormatter": "NicolasVuillamy.vscode-groovy-lint"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
